{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "appConfig": {
    "strategy": "TransformOnly",
    "disable": false
  },
  "artifactCopy": {
    "disable": false
  },
  "automaticVersioning": {
    "behavior": "PreferBuildNumber",
    "environment": "All",
    "versionOffset": 0,
    "disable": false
  },
  "css": {
    "minify": false,
    "bundleScss": false,
    "disable": false
  },
  "images": {
    "directories": null,
    "conditionalDirectories": null,
    "watermarkOpacity": null,
    "disable": false
  },
  "manifests": {
    "token": "$",
    "variablePrefix": "Manifest_",
    "missingTokensAsErrors": false,
    "disable": false
  },
  "releaseNotes": {
    "maxDays": 7,
    "maxCommit": 10,
    "characterLimit": 250,
    "filename": "ReleaseNotes.txt",
    "createInRoot": true,
    "disable": false
  },
  "projectSecrets": {
    "Covi.Configuration": {
      "disable": false,
      "delimiter": ";",
      "prefix": "BuildTools_",
      "className": "Secrets",
      "namespace": "Helpers",
      "properties": [
        {
          "name": "EndpointUrl",
          "type": "String",
          "isArray": false
        },
        {
          "name": "SecretsProvider_EncryptionKeyFieldName",
          "type": "String",
          "isArray": false
        },
        {
          "name": "SecretsProvider_DeviceIdentifierFieldName",
          "type": "String",
          "isArray": false
        },
        {
          "name": "SecretsProvider_InitializedFlagFieldName",
          "type": "String",
          "isArray": false
        },
        {
          "name": "SecretsProvider_IsFirstRunFieldName",
          "type": "String",
          "isArray": false
        },
        {
          "name": "AppCenter_iOS_Secret",
          "type": "String",
          "isArray": false
        },
        {
          "name": "AppCenter_Android_Secret",
          "type": "String",
          "isArray": false
        },
        {
          "name": "PushNotifications_NotificationChannelName",
          "type": "String",
          "isArray": false
        },
        {
          "name": "HybridEndpointUrl",
          "type": "String",
          "isArray": false
        },
        {
          "name": "PlatformSubscriptionKey",
          "type": "String",
          "isArray": false
        }
      ]
    }
  },
  "debug": false
}